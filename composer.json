{
    "name"             : "whizark/chosen",
    "description"      : "A common class/interface/trait library for PHP 5.4+",
    "type"             : "library",
    "keywords"         : [
        "library",
        "class",
        "interface",
        "trait"
    ],
    "homepage"         : "https://github.com/whizark/chosen",
    "license"          : "MIT",
    "minimum-stability": "stable",
    "authors"          : [
        {
            "name"     : "Whizark",
            "homepage" : "http://whizark.com",
            "role"     : "Developer"
        }
    ],
    "support"          : {
        "issues"       : "https://github.com/whizark/chosen/issues",
        "source"       : "https://github.com/whizark/chosen"
    },
    "require"          : {
        "php"          : ">=5.4.0"
    },
    "require-dev"      : {
        "phpunit/phpunit"       : "3.7.*",
        "mockery/mockery"       : "dev-master",
        "satooshi/php-coveralls": "dev-master",
        "apigen/apigen"         : "dev-master"
    },
    "suggest"          : {
        "ext-openssl" : "Enabling the openssl extension allows you to use a cryptographically secure pseudo-random number generator"
    },
    "autoload"         : {
        "psr-0"        : {
            "Chosen\\"  : "src/"
        }
    }
}
